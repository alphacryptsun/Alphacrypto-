  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-xfull"] > :nth-child(n)::before { left: calc(var(--inner-gutter) / -2); right: calc(var(--inner-gutter) / -2); }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-x"] > :nth-child(8n+1)::before { left: 0px; }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-x"] > :nth-child(8n+8)::before { right: 0px; }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-x"] > :nth-child(8n+1):nth-last-child(-n+8)::before { border-bottom-width: 0px; }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-x"] > :nth-child(8n+1):nth-last-child(-n+8) ~ ::before { border-bottom-width: 0px; }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-y"][class*="lstw-grid-line-y"] > :nth-child(n)::after { border-right-width: 1px; }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-y"][class*="lstw-grid-line-y"] > :nth-child(8n+8)::after { border-right-width: 0px; }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-y"][class*="lstw-grid-line-y"] > :nth-child(-n+8)::after { top: 0px; }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-y"][class*="lstw-grid-line-y"] > :nth-child(8n+1):nth-last-child(-n+8)::after { bottom: 0px; }
  .sm\:lstw-grid-cols-8[class*="lstw-grid-line-y"][class*="lstw-grid-line-y"] > :nth-child(8n+1):nth-last-child(-n+8) ~ li::after { bottom: 0px; }
}

