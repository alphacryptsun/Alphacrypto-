  .sm\:lstw-grid-line-x-0 > ::before { content: none; }
  .sm\:lstw-grid-line-y > ::after { content: attr(); left: 0px; right: calc(var(--inner-gutter) / -2); top: 0px; bottom: 0px; border-left: 0px solid transparent; border-right: 0px solid transparent; }
  .sm\:lstw-grid-line-yfull > ::after { content: attr(); left: 0px; right: calc(var(--inner-gutter) / -2); top: calc(var(--inner-gutter) / -1); bottom: 0px; border-left: 0px solid transparent; border-right: 0px solid transparent; }
