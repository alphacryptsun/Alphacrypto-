  .md\:lstw-grid-cols-6[class*="lstw-grid-line-x"] > :nth-child(6n+1)::before { left: 0px; }
  .md\:lstw-grid-cols-6[class*="lstw-grid-line-x"] > :nth-child(6n+6)::before { right: 0px; }
  .md\:lstw-grid-cols-6[class*="lstw-grid-line-x"] > :nth-child(6n+1):nth-last-child(-n+6)::before { border-bottom-width: 0px; }
  .md\:lstw-grid-cols-6[class*="lstw-grid-line-x"] > :nth-child(6n+1):nth-last-child(-n+6) ~ ::before { border-bottom-width: 0px; }
  .md\:lstw-grid-cols-6[class*="lstw-grid-line-y"][class*="lstw-grid-line-y"] > :nth-child(n)::after { border-right-width: 1px; }
  .md\:lstw-grid-cols-6[class*="lstw-grid-line-y"][class*="lstw-grid-line-y"] > :nth-child(6n+6)::after { border-right-width: 0px; }
  .md\:lstw-grid-cols-6[class*="lstw-grid-line-y"][class*="lstw-grid-line-y"] > :nth-child(-n+6)::after { top: 0px; }
