@media (min-width: 744px) {
  [class*="lstw-grid-line-"] > * { position: relative; }
  [class*="lstw-grid-line-"] > ::before, [class*="lstw-grid-line-"] > ::after { content: attr(); position: absolute; z-index: 0; pointer-events: none; }
  .md\:lstw-grid-line-x > ::before { content: attr(); left: 0px; right: 0px; top: 0px; bottom: calc(var(--inner-gutter) / -2); border-top: 0px solid transparent; border-bottom: 0px solid transparent; }
  .md\:lstw-grid-line-xfull > ::before { content: attr(); left: calc(var(--inner-gutter) / -2); right: calc(var(--inner-gutter) / -2); top: 0px; bottom: calc(var(--inner-gutter) / -2); border-top: 0px solid transparent; border-bottom: 0px solid transparent; }
